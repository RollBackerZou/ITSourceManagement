@model ITSourceManagement.ViewModel.ForgotPasswordViewModel
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title></title>
</head>
<body>
    <div>
    @using(Html.BeginForm("ForgetPsw","Account",FormMethod.Post))
